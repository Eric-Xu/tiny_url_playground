<h1>Registered Users: <%= @users.count %></h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Urls Saved</th>
      <th>Last Log In Date</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.urls.count %></td>
        <td><%= user.created_at.strftime('%m/%d/%Y %I:%M %p') %></td>
        <td><%= link_to '(delete)', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>