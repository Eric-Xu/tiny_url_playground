<ul>
	<li id="logo"><%= link_to 'Logo :D', root_url %></li>
</ul>
<ul>
	<% if current_user %>
		<li>Logged in as <%= current_user.email %></li>
		<li><%= link_to_unless_current "Log Out", logout_path %></li>
	<% else %>
		<li><%= link_to_unless_current "Join now. It's fast & free!", signup_path %></li>
		<li><%= link_to_unless_current "Log In", login_path %></li>
	<% end %>
</ul>